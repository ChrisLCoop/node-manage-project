<%-  include('includes/header') %>
            <div class="content-gen-dash">
                <h2>New Users</h2>
                <div class="content-form">
                    <form action="/users/create-user" method="post">
                        <input type="text" class="form-control" placeholder="Nombres" name="name" required><br>
                        <input type="text" class="form-control" placeholder="Apellidos" name="lastName" required><br>
                        <input type="text" class="form-control" placeholder="correo" name="email" required><br>
                        <input type="password" class="form-control" placeholder="contraseÃ±a" name="password" required><br>
                        <input type="text" class="form-control" placeholder="profession" name="profession" required><br>
                        <label for="" class="form-label">Category</label>
                        <select name="id_role" selected value id="" required>
                            <% data.forEach(function(row) { %>
                            <option value="<%= row.id_role %>"><%= row.name_role %></option>
                            <% }); %>
                        </select><br>
                        <button type="submit" class="btn btn-primary">Create</button>
